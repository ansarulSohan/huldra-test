{
  "env": {
    "browser": true,
    "es2021": true,
    "jest/globals": true
  },
  "extends": [
    "react-app",
    "react-app/jest",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "jest"],
  "rules": {
    "quotes": ["error", "double", { "allowTemplateLiterals": true, "avoidEscape": true }],
    "semi": ["error", "always"],
    "react/prop-types": "off",
    "react/display-name": "off",
    "react/react-in-jsx-scope": "off",
    "no-unused-vars": "warn",
    "no-undef": "warn",
    "no-unreachable": "error",
    "array-callback-return": 0,
    "constructor-super": 0,
    "for-direction": 0,
    "getter-return": 0,
    "no-async-promise-executor": 0,
    "no-await-in-loop": 0,
    "no-class-assign": 0,
    "no-compare-neg-zero": 0,
    "no-cond-assign": 0,
    "no-const-assign": 0,
    "no-constant-binary-expression": 0,
    "no-constant-condition": 0,
    "no-constructor-return": 0,
    "no-control-regex": 0,
    "no-debugger": 0,
    "no-dupe-args": 0,
    "no-dupe-class-members": 0,
    "no-dupe-else-if": 0,
    "no-dupe-keys": 0,
    "no-duplicate-case": 0,
    "no-duplicate-imports": 0,
    "no-empty-character-class": 0,
    "no-empty-pattern": 0,
    "no-ex-assign": 0,
    "no-fallthrough": 0,
    "no-func-assign": 0,
    "no-import-assign": 0,
    "no-inner-declarations": 0,
    "no-invalid-regexp": 0,
    "no-irregular-whitespace": 0,
    "no-loss-of-precision": 0,
    "no-misleading-character-class": 0,
    "no-new-native-nonconstructor": 0,
    "no-new-symbol": 0,
    "no-obj-calls": 0,
    "no-promise-executor-return": 0,
    "no-prototype-builtins": 0,
    "no-self-assign": 2,
    "no-self-compare": 2,
    "no-setter-return": 2,
    "no-sparse-arrays": 0,
    "no-template-curly-in-string": 0,
    "no-this-before-super": 0,
    "no-unexpected-multiline": 1,
    "no-unmodified-loop-condition": 0,
    "no-unreachable-loop": 1,
    "no-unsafe-finally": 0,
    "no-unsafe-negation": 0,
    "no-unsafe-optional-chaining": 1,
    "no-unused-private-class-members": 1,
    "no-use-before-define": 1,
    "no-useless-backreference": 1,
    "require-atomic-updates": 0,
    "use-isnan": 1,
    "valid-typeof": 1,
    "accessor-pairs": 0,
    "arrow-body-style": 1,
    "block-scoped-var": 1,
    "camelcase": 1,
    "capitalized-comments": 0,
    "class-methods-use-this": 0,
    "complexity": 0,
    "consistent-return": 0,
    "consistent-this": 0,
    "curly": 1,
    "default-case": 1,
    "default-case-last": 1,
    "default-param-last": 0,
    "dot-notation": 0,
    "eqeqeq": 1,
    "func-name-matching": 0,
    "func-names": 0,
    "func-style": 0,
    "grouped-accessor-pairs": 0,
    "guard-for-in": 0,
    "id-denylist": 0,
    "id-length": 0,
    "id-match": 0,
    "init-declarations": 0,
    "logical-assignment-operators": 0,
    "max-classes-per-file": 0,
    "max-depth": 0,
    "max-lines": 0,
    "max-lines-per-function": 0,
    "max-nested-callbacks": 0,
    "max-params": 0,
    "max-statements": 0,
    "multiline-comment-style": 0,
    "new-cap": 0,
    "no-alert": 0,
    "no-caller": 0,
    "no-case-declaration": 0,
    "no-confusing-arrow": 0,
    "no-console": 0,
    "no-continue": 0,
    "no-delete-var": 0,
    "no-div-regex": 0,
    "no-else-return": 0,
    "no-empty": 0,
    "no-empty-function": 1,
    "no-empty-static-block": 0,
    "no-eq-null": 0,
    "no-eval": 0,
    "no-extend-native": 0,
    "no-extra-bind": 0,
    "no-extra-boolean-case": 0,
    "no-extra-label": 0,
    "no-extra-semi": 0,
    "no-floating-decimal": 0,
    "no-global-assign": 0,
    "no-implicit-coercion": 0
  },
  "ignorePatterns": ["temp.js", "**/*.test.js", "build/", "node_modules/"]
}
